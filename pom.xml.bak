<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.angular</groupId>
	<artifactId>Angular</artifactId>
	<packaging>war</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>Angular Maven Webapp</name>
	<repositories>
		<repository>
			<id>JBoss repository</id>
			<url>http://repository.jboss.org/nexus/content/groups/public/</url>
		</repository>
	</repositories>
	
	<properties>
        <!--endorsed.dir>${project.build.directory}/endorsed</endorsed.dir-->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <spring-version>4.2.4.RELEASE</spring-version>
        <jackson.version>1.9.10</jackson.version>
        <hibernate.version>4.3.1.Final</hibernate.version>
        <hibernate.jpa.ver>1.0.0.Final</hibernate.jpa.ver>
        <commons.dbcp>1.2.2</commons.dbcp>
        <jstl.api>1.2.1</jstl.api>
        <jstl.version>1.2.2</jstl.version>
        <javaee.api>7.0</javaee.api>
        <persistence.api>1.0</persistence.api>
        <spring.session>1.0.2.RELEASE</spring.session>
    	</properties>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<!--dependency>
            		<groupId>org.springframework.session</groupId>
            		<artifactId>spring-session</artifactId>
           		 <version>${spring.session}</version>
        	</dependency-->
        	<dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-jdbc</artifactId>
            <version>${spring-version}</version>
        </dependency>
	</dependencies>
	<build>
		<finalName>Angular</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<url>http://localhost:8080/manager/text</url>
					<server>TomcatServer</server>
					<path>/Angular</path>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
